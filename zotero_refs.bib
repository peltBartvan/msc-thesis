@article{turkaySpreadingResistanceModeling2021,
  title = {Spreading Resistance Modeling for Rapid Extraction of Contact Resistivity with a Four-Point Probe},
  author = {Turkay, Deniz and Tsoi, Konstantin and Donercark, Ergi and Turan, Rasit and Yerci, Selcuk},
  year = {2021},
  month = sep,
  journal = {Solar Energy Materials and Solar Cells},
  volume = {230},

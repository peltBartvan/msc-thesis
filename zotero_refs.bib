@article{coxOhmicContactsGaAs1967,
  title = {Ohmic Contacts for {{GaAs}} Devices},
  author = {Cox, R. H. and Strack, H.},
  year = {1967},
  month = dec,
  journal = {Solid-State Electronics},
  volume = {10},
  number = {12},
  pages = {1213--1218},
  issn = {0038-1101},
  doi = {10.1016/0038-1101(67)90063-9},
  abstract = {Contact alloys were developed for use on a wide variety of GaAs devices such as high temperature transistors and Gunn oscillators. The alloys are composed of silver, indium and germanium for n-type GaAs and of silver, indium and zinc for p-type GaAs. Fabrication steps that require temperatures of up to 770\textdegree K for already contacted devices can be performed. GaAs transistors can be operated over a range from 20 to 770\textdegree K using Ag-In-Ge contacts for emitter and collector and Ag-In-Zn contacts for the base. Gunn oscillators have been built for the frequency range between 13 and 26 GHz with efficiencies as high as 3 percent at 15.8 GHz and as high as 1 percent at 25 GHz in continuous wave operation. A simple technique was developed to evaluate the specific contact resistance on thin epitaxial layers. Specific contact resistance is well below 10-4 {$\omega$}-cm2 on 0.1 {$\omega$}-cm or lower resistivity p- or n-type GaAs. The highest value was 1 \texttimes{} 10-3 {$\omega$}-cm2 measured on 0.6\textendash 2.6 {$\omega$}-cm n-type GaAs. R\'esum\'e Des alliages de contact ont \'et\'e d\'evelopp\'es pour \^etre employ\'es dans une grande vari\'et\'e de dispositifs AsGa tels que les transistors \`a haute temp\'erature et les oscillateurs Gunn. Ces alliages sont compos\'es d'argent, d'indium et de germanium pour l'AsGa de type n et d'argent, indium et zinc pour l'AsGa de type p. Les \'etapes de la fabrication qui n\'ecessitent des temp\'eratures atteignant 770\textdegree K pour des dispositifs \`a contact peuvent \^etre obtenues. Les transistors AsGa peuvent \^etre op\'er\'es le long d'une gamme de 20 \`a 770\textdegree K employant des contacts Ag-In-Ge pour l'\'emetteur et le collecteur et des contacts Ag-In-Zn pour la base. Des oscillateurs Gunn ont \'et\'e construits pour la gamme de fr\'equence entre 13 et 26 GHz ayant des rendements aussi \'elev\'es que 3\% \`a 15,8 GHz et 1\% \`a 25GHz \`a regime d'ondes continues. Une simple technique a \'et\'e d\'evelopp\'ee pour \'evaluer la r\'esistance sp\'ecifique de contact sur de fines couches \'epitaxi\'ees. La r\'esistance sp\'ecifique de contact est bien au-dessous de 10-4 {$\omega$}-cm2 sur l'AsGa de type P et N ayant une r\'esistivit\'e de 0,1 {$\omega$}-cm au moins. La valeur maximum etait 1 \texttimes{} 10-3 {$\omega$}-cm2 mesur\'ee sur l'AsGa de type N\`a 0,2\textendash 2,6 {$\omega$}-cm. Zusammenfassung Es wurden Legierungen zur Kontaktierung verschiedener GaAs-Bauelemente entwickelt, wie z.B. f\"ur Hochtemperatur GaAs-Transistoren und Gunn-Oszillatoren. Die Legierung besteht aus Silber, Indium und Germanium f\"ur n-leitendes GaAs und aus Silber, Indium und Zink f\"ur p-leitendes GaAs. Herstellungsprozesse, die Temperaturen bis 770\textdegree K erfordern, k\"onnen an den bereits kontaktierten Elementen durchgef\"uhrt werden. GaAs-Transistoren k\"onnen \"uber einen Temperaturbereich von 20 bis 770\textdegree K betrieben werden, wenn Ag-In-Ge Kontakte f\"ur den Emitter und Ag-In-Zn Kontakte f\"ur die Basis verwendet werden. Es wurden Gunn-Oszillatoren mit einem Wirkungsgrad bis zu 3 Prozent bei einer Frequenz von 15,8 GHz und bis zu 1 Prozent bei einer Frequenz von 25 GHz in Dauerstrich-Betrieb gebaut. Eine einfache Methode, den spezifischen Kontaktwiderstand auf d\"unnen Epitaxie-Schichten zu ermitteln wurde entwickelt. Der spezifische Kontaktwiderstand ist niedriger als 10-4 {$\omega$}-cm2 auf 0,1 {$\omega$}-cm und h\"oher dotierten p- oder n-leitendem GaAs. Der h\"ochste Wert betrug 1 \texttimes{} 10-3 {$\omega$}-cm2 auf 0,6\textendash 2,6 {$\omega$}-cm n-leitendem GaAs.},
  langid = {english},
  file = {/home/bart/Zotero/storage/2KW72ASX/0038110167900639.html}
}

@article{delalamoPhysicsModelingHeavily1984,
  title = {The Physics and Modeling of Heavily Doped Emitters},
  author = {{del Alamo}, J.A. and Swanson, R.M.},
  year = {1984},
  month = dec,
  journal = {IEEE Transactions on Electron Devices},
  volume = {31},
  number = {12},
  pages = {1878--1888},
  issn = {1557-9646},
  doi = {10.1109/T-ED.1984.21805},
  abstract = {The physics of minority-carrier injection and internal quantum efficiency of heavily doped emitters is studied through a novel computer simulation. It is shown that in the shallow emitters of modern devices, the transport of carriers through the bulk of the emitter, and the surface recombination rate are the dominant mechanisms controlling the minority-carrier profile. Carrier recombination in the bulk of the emitter only produces a small perturbation of this profile. This observation permits us to develop a simple and accurate analytical model for the saturation current and internal quantum efficiency of shallow emitters.}
}

@article{dingSolutionprocessedZnOEfficient2018,
